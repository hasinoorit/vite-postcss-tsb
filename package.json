{
  "name": "vite-postcss-tsb",
  "version": "1.0.6",
  "description": "This plugin enable vite to build css using postcss without bundling into single file",
  "main": "./index.cjs",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.cjs"
    }
  },
  "type": "module",
  "scripts": {
    "build": "npx unbuild"
  },
  "keywords": [
    "vite",
    "css",
    "postcss"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hasinoorit/vite-postcss-tsb.git"
  },
  "homepage": "https://github.com/hasinoorit/vite-postcss-tsb",
  "author": "Hasinoor Rahman",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.2.5",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1",
    "vite": "^4.3.9"
  },
  "peerDependencies": {
    "postcss": ">=8",
    "postcss-preset-env": ">=8",
    "vite": ">=2.9.0"
  }
}